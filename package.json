{
  "private": true,
  "name": "nishans",
  "version": "0.0.20",
  "description": "An ecosystem of packages for notion written in typescript",
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.17",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.17.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-jest": "^24.1.3",
    "husky": "^5.1.3",
    "jest": "26.6.0",
    "lerna": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "start": "tsc --build tsconfig.json --watch",
    "prebuild": "npm run lint && run-s clean",
    "prestart": "run-s clean",
    "clean": "del-cli packages/*/dist",
    "dev": "run-s start",
    "pretest": "run-s build",
    "test": "run-s test:*",
    "test:unit": "lerna run test",
    "bootstrap": "lerna bootstrap",
    "publish": "lerna publish",
    "lint": "npx eslint . --color",
    "preinstall": "node -e \"if (process.env.npm_execpath.indexOf('yarn') < 0) throw new Error('this package requires yarn for development')\""
  },
  "keywords": [],
  "author": "Safwan Shaheer <devorein00@gmail.com>",
  "license": "MIT"
}
